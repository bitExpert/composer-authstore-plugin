{
    "name" : "bitexpert/composer-authstore-plugin",
    "description" : "Composer Plugin providing an external auth store for storing your access credentials.",
    "type" : "composer-plugin",
    "keywords" : [
        "composer",
        "auth.json"
    ],
    "homepage" : "https://github.com/bitExpert/composer-authstore-plugin",
    "license" : "Apache-2.0",
    "authors" : [{
            "name" : "Stephan Hochd√∂rfer",
            "email" : "S.Hochdoerfer@bitExpert.de"
        }
    ],
    "autoload" : {
        "psr-0" : {
            "bitExpert\\Composer\\Auth" : [
                "src/",
                "tests/"
            ]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "class" : "\\bitExpert\\Composer\\Auth\\AuthStorePlugin"
    },
    "require" : {
        "composer-plugin-api" : "1.0.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "4.0.*@stable",
        "composer/composer" : "1.*@dev",
        "mikey179/vfsStream" : "v1.2.0"
    }
}
